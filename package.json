{
  "name": "@wont/biz-ui",
  "version": "1.2.2",
  "description": "A react library developed with dumi",
  "homepage": "wont-org.github.io/biz-ui/",
  "bugs": {
    "url": "https://github.com/wont-org/biz-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wont-org/biz-ui"
  },
  "license": "MIT",
  "author": {
    "name": "liukun",
    "email": "919590347@qq.com"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "build": "father build",
    "build:docs": "dumi build",
    "build:watch": "father dev",
    "bump": "bumpp package.json -c -p -t --all -x \"npm run changelog\"",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "dev": "dumi dev",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "is-ci || husky install && dumi setup",
    "prepublishOnly": "npm run bump &&father doctor && npm run build",
    "start": "npm run dev"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "**/*.{js,jsx}": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write"
    ],
    "**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ],
    "!public/js/**/*": [],
    "!public/data/**/*": []
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@ant-design/pro-components": "^2.6.49",
    "@babel/runtime": "^7.24.5",
    "@wont/use-antd-resizable-header": "1.0.3",
    "ahooks": "^3.7.8",
    "chroma-js": "^3.1.2",
    "classnames": "^2.3.2",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "react-color": "^2.19.3",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/chroma-js": "^3.1.1",
    "@types/lodash": "^4.14.202",
    "@types/react": "^16.9.0",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^16.9.0",
    "@umijs/lint": "^4.0.0",
    "antd": "^4.23.0",
    "babel-plugin-import": "^1.13.8",
    "babel-plugin-styled-components": "^2.1.4",
    "bumpp": "^9.4.0",
    "conventional-changelog-cli": "^4.1.0",
    "dumi": "2.2.13",
    "eslint": "^8.23.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "father": "4.1.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "stylelint": "^14.9.1"
  },
  "peerDependencies": {
    "antd": "^4.23.0 || ^5.0.0",
    "moment": "2.30.1",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "authors": []
}
