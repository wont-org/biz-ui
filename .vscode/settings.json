{
  "eslint.validate": ["javascript", "javascriptreact", "typescriptreact", "typescript", "html"],
  "stylelint.validate": [
    "css",
    "less",
    "scss",
    "postcss"
    // "typescriptreact",
    // "typescript"
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  }
}
